<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<entry key="insertMember">
		INSERT INTO MEMBER 
		VALUES(
			SEQ_MEMNO.NEXTVAL, 
			?, 
			?, 
			?, 
			?, 
			?, 
			?
		)
	</entry>
	<entry key="selectMemberList">
		SELECT * 
		FROM MEMBER
	</entry>
	<entry key="updateMember">
		UPDATE MEMBER 
		SET 
			USER_NICKNAME=?,
			USER_EMAIL=?
		WHERE USER_ID=?
	</entry>
	<entry key="deleteMember">
		DELETE FROM MEMBER 
		WHERE USER_ID = ?
	</entry>
	<entry key="keywordSerch">
		SELECT * 
		FROM MEMBER 
		WHERE USER_NAME LIKE '%' || ? || '%'
	</entry>
	<entry key="userIdValidation">
		SELECT USER_ID 
		FROM MEMBER 
		WHERE USER_ID=?
	</entry>
		<entry key="allInsertMember">
		INSERT INTO MEMBER 
		VALUES(
			SEQ_USERNO.NEXTVAL, 
			?, 
			?, 
			?, 
			?, 
			?, 
			?, 
			?, 
			?, 
			?, 
			SYSDATE
		)
	</entry>
	<entry key="allDeleteMember">
		DELETE FROM MEMBER 
		WHERE USER_ID = ?
	</entry>

	<entry key="loginMember">SELECT * 
		FROM MEMBER 
		WHERE USER_ID = ? AND USER_PW = ?</entry>
</properties>
